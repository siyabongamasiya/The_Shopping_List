{
  "expo": {
    "name": "Shopping List",
    "slug": "The_Shopping_List",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/shopping-list-logo.png",
    "scheme": "theshoppinglist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/shopping-list-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.siyabongakhanyile.shoppinglist"
    },
    "android": {
      "package": "com.siyabongakhanyile.shoppinglist",
      "permissions": [],
      "adaptiveIcon": {
        "backgroundColor": "#2563eb",
        "foregroundImage": "./assets/images/shopping-list-logo.png"
      },
      "jsEngine": "hermes",
      "buildToolsVersion": "34.0.0",
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "minSdkVersion": 24
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/shopping-list-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9eb74521-c2c2-4326-a445-e151322a24d2"
      }
    }
  }
}
